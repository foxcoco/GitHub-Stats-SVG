{
  "functions": {
    "api/*.js": {
      "memory": 256,
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/gh0stintheshe11/github-stats-svg"
    }
  ],
  "rewrites": [
    {
      "source": "/api/github-status",
      "destination": "/api/index.js"
    }
  ]
}