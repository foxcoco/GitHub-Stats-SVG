{
  "functions": {
    "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/anuraghazra/github-readme-stats"
    }
  ],
  "rewrites": [
    {
      "source": "/api/github-status",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/github-languages",
      "destination": "/api/index.js"
    }
  ]
}

